<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/linearLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/green">


    <TextView
        android:id="@+id/artistName1"
        android:layout_constraintEnd_toEndOf="parent"
        android:layout_constraintStart_toStartOf="parent"
        android:layout_constraintTop_toTopOf="parent"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Here are your top artists!"
        android:textAlignment="center"
        tools:layout_editor_absoluteX="128dp"
        tools:layout_editor_absoluteY="46dp" />

    <TextView
        android:id="@+id/artistName1"
        android:layout_constraintEnd_toEndOf="parent"
        android:layout_constraintStart_toStartOf="parent"
        android:layout_constraintTop_toTopOf="parent"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Artist 1"
        android:textAlignment="center"
        tools:layout_editor_absoluteX="182dp"
        tools:layout_editor_absoluteY="194dp" />

    <TextView
        android:id="@+id/artistName2"
        android:layout_constraintEnd_toEndOf="parent"
        android:layout_constraintStart_toStartOf="parent"
        android:layout_constraintTop_toBottomOf="@id/artistName1"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Artist 2"
        android:textAlignment="center"
        tools:layout_editor_absoluteX="182dp"
        tools:layout_editor_absoluteY="356dp" />

    <TextView
        android:id="@+id/artistName3"
        android:layout_constraintEnd_toEndOf="parent"
        android:layout_constraintStart_toStartOf="parent"
        android:layout_constraintTop_toBottomOf="@id/artistName2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Artist 3"
        android:textAlignment="center"
        tools:layout_editor_absoluteX="182dp"
        tools:layout_editor_absoluteY="550dp" />

</androidx.constraintlayout.widget.ConstraintLayout>